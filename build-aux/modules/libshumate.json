{
    "buildsystem": "meson",
    "config-opts": [
        "-Ddemos=false",
        "-Dgtk_doc=false",
        "-Dvapi=false"
    ],
    "modules": [
        {
            "buildsystem": "autotools",
            "config-opts": [
                "--disable-protoc"
            ],
            "name": "protobuf-c",
            "sources": [
